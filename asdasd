[32m+[m[32m# Webhook route for fulfillment[m
[32m+[m[32m@app.route('/webhook', methods=['POST'])[m
[32m+[m[32mdef webhook():[m
[32m+[m[32m    req = request.get_json(silent=True, force=True)[m
[32m+[m[32m    intent_name = req['queryResult']['intent']['displayName'][m
[32m+[m
[32m+[m[32m    # Handle Study Timer intent[m
[32m+[m[32m    if intent_name == "Study Timer":[m
[32m+[m[32m        duration = req['queryResult']['parameters'].get('duration', None)[m
[32m+[m
[32m+[m[32m        # Check if duration was provided[m
[32m+[m[32m        if duration:[m
[32m+[m[32m            duration_seconds = int(duration['amount']) * 60  # Convert minutes to seconds[m
[32m+[m[32m            response_text = f"Great! Iâ€™ve set a {duration['amount']} {duration['unit']} study timer. Stay focused!"[m
[32m+[m[32m        else:[m
[32m+[m[32m            response_text = "How long should I set the study timer for?"[m
[32m+[m
[32m+[m[32m    # Fallback if intent is not recognized[m
[32m+[m[32m    else:[m
[32m+[m[32m        response_text = "Hmm... I didnâ€™t quite get that. Can you try again?"[m
[32m+[m
[32m+[m[32m    # Return the response[m
[32m+[m[32m    return jsonify({"fulfillmentText": response_text})[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    app.run(debug=True)[m
